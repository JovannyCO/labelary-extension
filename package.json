{
  "name": "labelary",
  "displayName": "Labelary and PDF viewer",
  "description": "View Labelary (ZPL), PDF and other base64-encoded formats in separate panel on right-mouse-button click.",
  "license": "MIT",
  "version": "1.0.5",
  "publisher": "RoelKneepkens-ShipitSmarter",
  "author": {
    "name": "Roel Kneepkens",
    "url": "https://github.com/roelkneepkens"
  },
  "contributors": [
    {
      "name": "Bram van der Horn",
      "url": "https://github.com/bvanderhorn"
    },
    {
      "name": "Wouter Keizer",
      "url": "https://github.com/WouterKeizer"
    }
  ],
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "ShipitSmarter",
    "Labelary",
    "ZPL",
    "PDF",
    "Base64",
    "Viewer"
  ],
  "activationEvents": [
    "onCommand:labelary.view-label4x8",
    "onCommand:labelary.view-label8x4",
    "onCommand:labelary.view-label8.25x11.75"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "labelary.view-label4x8",
        "title": "View 4x8"
      },
      {
        "command": "labelary.view-label8x4",
        "title": "View 8x4"
      },
      {
        "command": "labelary.view-label8.25x11.75",
        "title": "View A4"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "labelarysubmenu",
          "group": "labelary"
        }
      ],
      "labelarysubmenu": [
        {
          "command": "labelary.view-label4x8",
          "when": "editorTextFocus || editorHasSelection || editorReadonly || isInDiffEditor || isInEmbeddedEditor",
          "group": "labelary"
        },
        {
          "command": "labelary.view-label8x4",
          "when": "editorTextFocus || editorHasSelection || editorReadonly || isInDiffEditor || isInEmbeddedEditor",
          "group": "labelary"
        },
        {
          "command": "labelary.view-label8.25x11.75",
          "when": "editorTextFocus || editorHasSelection || editorReadonly || isInDiffEditor || isInEmbeddedEditor",
          "group": "labelary"
        }
      ]
    },
    "submenus": [
      {
        "label": "View label via labelary",
        "id": "labelarysubmenu"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/node-fetch": "^2.6.1",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "filereader": "^0.10.3",
    "node-fetch": "^2.6.1",
    "mime": "2.4.6",
    "pdfjs-dist": "2.4.456",
    "xss": "1.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roelkneepkens/labelary-extension"
  },
  "icon": "img/labelary-logo.png"
}